{
  "name": "Club",
  "plural": "Clubs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "web_page": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "administrator": {
      "type": "belongsTo",
      "model": "People",
      "foreignKey": "administrator_id"
    },
    "races": {
      "type": "hasMany",
      "model": "Race",
      "foreignKey": ""
    },
    "participants": {
      "type": "hasMany",
      "model": "Participant",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
